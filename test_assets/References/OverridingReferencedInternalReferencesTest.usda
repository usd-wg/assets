#usda 1.0
(
    defaultPrim = "World"
    endTimeCode = 100
    metersPerUnit = 0.01
    startTimeCode = 0
    subLayers = [
        @./utils/Environment.usda@
    ]
    timeCodesPerSecond = 24
    upAxis = "Z"
)

def Xform "World" (
    kind = "assembly"
)
{
    def "ComponentWithInternalReferences_01" (
        prepend references = @./OverridingReferencedInternalReferencesTest/ComponentWithInternalReferences.usda@
    )
    {
        over "Prefabs"
        {
            over "bolt"
            {
                over "Looks"
                {
                    over "copper"
                    {
                        over "Shader"
                        {
                            color3f inputs:diffuseColor = (1, 0, 0) (
                                hidden = false
                                renderType = "color"
                            )
                        }
                    }
                }
            }
        }
    }
}

